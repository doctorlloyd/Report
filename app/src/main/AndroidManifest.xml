<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.report" >
    <application
        android:allowBackup="true"
        android:icon="@drawable/logohand"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AlertDialog.AppCompat.Light" >
        <activity android:name=".RegistrationScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditScreen"
            android:label="Updating"
            android:icon="@drawable/logohand">
            <intent-filter>
                <action android:name="android.intent.action.UPDATE" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Result"
            android:label="Students"
            android:icon="@drawable/logohand">
            <intent-filter>
                <action android:name="android.intent.action.RESULT" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>
</manifest>